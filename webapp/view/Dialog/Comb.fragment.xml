<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
    <SelectDialog
        id="valueHelpDialog"
        title="Buseo"
        items="{ path: 'valueHelpModel>/' }"
        search="_handleValueHelpSearch"
        confirm="_handleValueHelpClose"
        cancel="_handleValueHelpClose"
        rememberSelections="true"
        multiSelect="true">
        <StandardListItem
            iconInset="false"
            title="{valueHelpModel>CombCode}"
            description="{valueHelpModel>CombName}"
            />
    </SelectDialog>
</core:FragmentDefinition>